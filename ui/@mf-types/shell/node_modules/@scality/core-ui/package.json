    "react-window": "^1.8.6",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "vega": "^5.17.3",
    "vega-embed": "6.0.0",
    "vega-lite": "5.0.0",
    "vega-tooltip": "0.27.0"
  },
  "homepage": "https://scality.github.io/core-ui/",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/scality/core-ui.git"
}
