  "baseUrl": "http://localhost:8084",
  "viewportWidth": 1440,
  "viewportHeight": 900,
  "defaultCommandTimeout": 30000,
  "experimentalNetworkStubbing": true,
  "experimentalFetchPolyfill": true
}
